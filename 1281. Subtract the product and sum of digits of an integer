//  https://leetcode.com/problems/subtract-the-product-and-sum-of-digits-of-an-integer/submissions/1753357903

class Solution {
public:
    int subtractProductAndSum(int n) {
        int pro=1;
        int sum=0;
        while(n!=0){
            int digit=n%10;
            pro=pro*digit;
            sum=sum+digit;
            n=n/10;
        }
        return pro-sum;
    }
};
