// https://leetcode.com/problems/excel-sheet-column-number/submissions/1933006700

class Solution {
public:
    int titleToNumber(string columnTitle) {
        int ans=0;
        int count=0;
        for(int i=columnTitle.length()-1;i>=0;i--){
            ans=ans+(pow(26,count)*(columnTitle[i]-'A'+1));
            count++;
        }
        return ans;
    }
};
