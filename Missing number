// https://leetcode.com/problems/missing-number/submissions/1918273109

class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int sum=0;
        for(int i=0;i<=nums.size();i++){
            sum+=i;
        }
        int arr_sum=0;
        for(int i=0;i<nums.size();i++){
            arr_sum+=nums[i];
        }
        return sum-arr_sum;
    }
};
