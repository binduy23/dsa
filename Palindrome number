// https://leetcode.com/problems/palindrome-number/submissions/1888041900

class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0){
            return false;
        }
        long int y=0;
        int z=x;
        while(z!=0){
            y=(y*10)+(z%10);
            z=z/10;
        
        }
        if(x==y){
            return true;
        }
        return false;
    }
};
