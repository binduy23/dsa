// https://leetcode.com/problems/remove-duplicates-from-sorted-array/submissions/1892323885

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int i=0;
        int j=1;
        cout<<nums.size();
        while(j<nums.size()){
            if(nums[j]==nums[i]){
                j++;
            }
            else{
                i++;
                nums[i]=nums[j];
                
            }
        }
        return i+1;
    }
};
