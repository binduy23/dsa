#include <bits/stdc++.h> 
void getAns(string str,vector<string>& ans,string output,int index){
  if(index>=str.length()){
    if(output.length()>0){
      ans.push_back(output);
    }
    
    return;
  }
  char ch=str[index];
  getAns(str,ans,output,index+1);
  output.push_back(ch);
  getAns(str,ans,output,index+1);
}
vector<string> subsequences(string str){
	
	// Write your code here
  vector<string> ans;
  string output="";
  int index=0;
  getAns(str,ans,output,index);
	return ans;
}
