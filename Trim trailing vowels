//https://leetcode.com/contest/weekly-contest-491/problems/trim-trailing-vowels/submissions/1934294577/

class Solution {
public:
    string trimTrailingVowels(string s) {
        for(int i=s.length()-1;i>=0;i--){
            while(i>=0 && (s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u')){
                s.erase(i,1);
                i--;
            }
            break;
        }
        return s;
    }
};Â©leetcode
