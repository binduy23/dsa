//     https://bit.ly/3Dm4hEE 


int floorSqrt(int n)
{
   int start=0;
   int end=(n/2)+1;
   long long int mid;
   long long int ans;
   while(start<=end){
       mid=start+(end-start)/2;

       if(mid*mid==n){
           return mid;
       }
       else if(mid*mid<n){
           start=mid+1;
           ans=mid;
       }
       else{
           end=mid-1;
       }
   } 
   return ans;// Write your code here.
}
